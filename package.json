{
  "name": "@telicent-oss/telicent-paralog",
  "version": "1.2.0",
  "private": true,
  "homepage": "paralog",
  "license": "See LICENSE",
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.3.0",
    "@tanstack/react-query": "^5.18.1",
    "@tanstack/react-query-devtools": "^5.18.1",
    "@telicent-oss/ds": "^0.0.3",
    "@telicent-oss/ontologyservice": "0.0.6",
    "@turf/turf": "^6.5.0",
    "classnames": "^2.3.1",
    "color-scales": "^3.0.2",
    "cytoscape": "^3.19.1",
    "cytoscape-avsdf": "^1.0.0",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-dagre": "^2.4.0",
    "cytoscape-node-html-label": "^1.2.2",
    "jsdom": "^26.0.0",
    "lodash": "^4.17.21",
    "mapbox-gl-draw-geodesic": "^2.1.3",
    "maplibre-gl": "^2.4.0",
    "primeicons": "^6.0.1",
    "primereact": "^9.2.1",
    "re-resizable": "^6.9.9",
    "react": "^18.2.0",
    "react-checkbox-tree": "^1.8.0",
    "react-cytoscapejs": "^1.2.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-map-gl": "^7.0.15",
    "react-modal": "^3.16.1",
    "react-switch": "^7.0.0",
    "react-tabs": "4.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "supercluster": "^8.0.1",
    "use-http": "^1.0.27",
    "use-supercluster": "^0.4.0",
    "vite": "^6.2.2",
    "vitest": "^3.0.8",
    "web-vitals": "^2.1.4",
    "zod": "3.23.8"
  },
  "scripts": {
    "dev": "PORT=3001 vite",
    "prestart": "[ \"$LOCAL_MACHINE\" = \"false\" ] && echo 'skip cp-config' || ./scripts/cp-config.js.sh",
    "build": "vite build",
    "preview": "vite preview",
    "build:tailwind": "npx tailwindcss -i ./src/index.css -o ./src/main.css",
    "postbuild": "[ \"$LOCAL_MACHINE\" = \"false\" ] && echo 'skip cp-config' || ./scripts/cp-config.js.sh",
    "babel": "babel --plugins @babel/plugin-transform-arrow-functions script.js",
    "test": "vitest",
    "test:ci": "vitest --ci --json --outputFile=\"reports/results.json\" --watchAll=false",
    "test:diff": "NODE_OPTIONS=--experimental-vm-modules yarn jest --watchAll=false --coverage=false --onlyChanged --bail",
    "test:vuln": "trivy fs --security-checks vuln --ignore-unfixed --severity HIGH,CRITICAL .",
    "bump:prerelease": "yarn version --prerelease && git push && git push --tags",
    "bump:pre": "yarn bump:prerelease",
    "prepare": "husky install",
    "postinstall": "[ \"$LOCAL_MACHINE\" = \"false\" ] && echo 'Skipping tefe hook-postinstall' || tefe hook-postinstall"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "**/*.html"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not safari < 10",
      "not chrome < 51",
      "not android < 5",
      "not ie < 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.25.9",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@telicent-oss/telicent-frontend-cli": "1.1.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.4",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-image-snapshot": "^4.5.1",
    "msw": "^0.49.3",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.1.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "resolutions": {
    "body-parser": "^1.20.3",
    "cross-spawn": "^7.0.6",
    "@babel/traverse": "7.25.9",
    "http-proxy-middleware": "^2.0.7",
    "msw/path-to-regexp": "6.3.0",
    "react-scripts/webpack-dev-server/express/path-to-regexp": "1.9.0",
    "**/react-scripts/**/path-to-regexp": "1.9.0",
    "braces": "3.0.3",
    "nth-check": "2.0.1",
    "webpack-dev-middleware": "5.3.4",
    "webpack": "5.76.0",
    "// ws ": "Found \"jsdom#ws@7.5.9\" jest-config#jest-environment-jsdom#jsdom - not worried as its just tests",
    "ws": "8.17.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!mapbox-gl-draw-geodesic)/"
    ]
  }
}
